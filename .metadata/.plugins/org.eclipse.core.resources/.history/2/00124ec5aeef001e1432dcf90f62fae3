public class Cgpa
{
	public static void main(String args[])
	{
		double y_marks[] = new double[4];
		y_marks[0] = 9.08;
		y_marks[1] = 9.18;
		y_marks[2] = 9.35;
		y_marks[3] = 9.6;
		
		
		Student123 s = new Student123("Bappi",20,y_marks);
		
		System.out.println(s.award_class());
	}
}