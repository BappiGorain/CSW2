interface Cgpa_Student
{
	double calc_cgpa_students(Student123 s);
}

class Student123
{
	String name;
	int age;
	double []y_marks;
	
	public Student123(String name, int age, double[] y_marks)
	{
		super();
		this.name = name;
		this.age = age;
		this.y_marks = y_marks;
	}

	public int award_class()
	{
		double cgpa = 0;
		for(int i=0;i<y_marks.length;i++)
		{
			cgpa += y_marks[i];
		}
		cgpa/=4;
		
		if(cgpa > 8)
		{
			return 1;
		}
		else if(cgpa<=8 && cgpa>=7.5)
		{
			return 2;
		}
		else
		{
			return 3;
		}
		
	}
}

